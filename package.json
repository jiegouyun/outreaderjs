{
  "name": "outreader",
  "scripts": {
    "lint": "prettier --write packages/**/*.{ts,js}",
    "lint:check": "prettier --check packages/**/*.{ts,js}",
    "test:yjk": "cd packages/outreader-yjk && yarn test",
    "dev:client": "cd packages/outreader-client && yarn dev",
    "build": "lerna run build",
    "cli": "./packages/outreader-cli/bin/run",
    "migrate:make": "knex migrate:make --migrations-directory=./packages/outreader-core/src/db/migrations -x ts",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "knex": "^0.21.1",
    "lerna": "^3.22.0",
    "typescript": "^3.9.5",
    "vuepress": "^1.5.0"
  },
  "dependencies": {
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --uncommitted"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "author": {
    "name": "an-lee",
    "email": "an.lee.work@gmail.com",
    "url": "https://outreader.com"
  },
  "contributors": [
    {
      "name": "ym6",
      "email": "yangml1988@gmail.com",
      "url": "https://github.com/ym6"
    }
  ],
  "bugs": {
    "url": "https://github.com/jiegouyun/outreaderjs/issues"
  },
  "homepage": "https://outreader.com",
  "private": true,
  "license": "Apache-2.0"
}
