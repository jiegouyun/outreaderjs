{
  "name": "@outreader/cli",
  "description": "A CLI tool to read output files",
  "version": "0.0.7",
  "author": "an-lee <an.lee.work@gmail.com>",
  "bin": {
    "outreader": "./bin/run"
  },
  "bugs": "https://github.com/an-lee/outreaderjs/issues",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.16.0",
    "@oclif/plugin-help": "^3.1.0",
    "@outreader/core": "^0.0.7",
    "@outreader/yjk": "^0.0.7",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4",
    "@types/mocha": "^8.0.0",
    "@types/node": "^16.11.12",
    "chai": "^4",
    "eslint": "^7.5.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^11.0.1",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": ["/bin", "/lib", "/npm-shrinkwrap.json", "/oclif.manifest.json"],
  "homepage": "https://github.com/an-lee/outreaderjs",
  "keywords": ["oclif"],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "outreader",
    "plugins": ["@oclif/plugin-help"]
  },
  "repository": "outreaderjs/outreaderjs",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack-backup": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
