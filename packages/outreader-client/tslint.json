{
  "defaultSeverity": "warning",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "max-line-length": {
      "options": [120]
    },
    "quotemark": [true, "single", "avoid-escape"],
    "variable-name": {
      "options": [
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    },
    "object-literal-key-quotes": [true, "as-needed"]
  },
  "rulesDirectory": []
}
