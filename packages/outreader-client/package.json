    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-function-bind": "^7.12.1",
    "@babel/plugin-proposal-function-sent": "^7.12.1",
    "@babel/plugin-proposal-json-strings": "^7.12.1",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-numeric-separator": "^7.12.5",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-proposal-pipeline-operator": "^7.12.1",
    "@babel/plugin-proposal-throw-expressions": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-react-constant-elements": "^7.12.1",
    "@babel/plugin-transform-react-inline-elements": "^7.10.4",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.6",
    "@types/jest": "^26.0.15",
    "@types/node": "^18.11.18",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-test-renderer": "^16.9.2",
    "@types/sinon": "^9.0.4",
    "@types/tapable": "^1.0.6",
    "@types/vfile-message": "^2.0.0",
    "@types/webpack": "^4.41.24",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "browserslist-config-erb": "^0.0.1",
    "chalk": "^4.1.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "cross-spawn": "^7.0.3",
    "css-loader": "^5.0.1",
    "detect-port": "^1.3.0",
    "electron": "11.0.3",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-rebuild": "^2.3.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.5.0",
    "fbjs-scripts": "^3.0.0",
    "file-loader": "^6.2.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.0",
    "node-sass": "^5.0.0",
    "opencollective-postinstall": "^2.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.5",
    "sinon": "^9.2.1",
    "spectron": "^12.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "testcafe": "^1.9.4",
    "testcafe-browser-provider-electron": "^0.0.15",
    "testcafe-react-selectors": "^4.1.0",
    "typed-css-modules-webpack-plugin": "^0.2.0",
    "typescript": "^4.0.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.3.0",
    "yarn": "^1.22.10"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@hot-loader/react-dom": "^17.0.0",
    "@outreader/core": "^0.0.7",
    "@outreader/pkpm": "^0.0.7",
    "@outreader/yjk": "^0.0.7",
    "@types/lowdb": "^1.0.9",
    "@types/recharts": "^1.8.16",
    "ali-react-table": "^2.1.1",
    "antd": "^4.8.2",
    "core-js": "^3.7.0",
    "devtron": "^1.4.0",
    "electron-debug": "^3.1.0",
    "electron-log": "^4.3.0",
    "electron-updater": "^4.3.1",
    "history": "^5.0.0",
    "lowdb": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.12.21",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "recharts": "^1.8.5",
    "source-map-support": "^0.5.19"
  },
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  },
  "browserslist": ["extends browserslist-config-erb"],
  "prettier": {
    "overrides": [
      {
        "files": [".prettierrc", ".babelrc", ".stylelintrc"],
        "options": {
          "parser": "json"
        }
      }
    ],
    "singleQuote": true
  },
  "stylelint": {
    "extends": ["stylelint-config-standard", "stylelint-config-prettier"]
  },
  "renovate": {
    "extends": ["bliss"]
  },
  "publishConfig": {
    "access": "public"
  }
}
